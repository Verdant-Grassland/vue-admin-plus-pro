{
  "compilerOptions": {
    "target": "ESNext",
    "composite": true,
    "noEmit": true,
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.node.tsbuildinfo",
    "skipLibCheck": true,
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "types": [
      "node"
    ],
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ]
    }
  },
  "extends": "@tsconfig/node20/tsconfig.json",
  "include": [
    "vite.config.*",
    "src/**/*",
    "src/**/*.vue",
    "types/*.ts"
  ],
  "exclude": [
    "src/**/__tests__/*"
  ]
}